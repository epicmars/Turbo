<resources>

    <style name="TurboAppTheme" parent="Theme.AppCompat.Light.DarkActionBar" >
        <item name="colorPrimary">@android:color/white</item>
        <item name="colorPrimaryDark">@android:color/white</item>
        <item name="colorAccent">@android:color/white</item>
    </style>

    <style name="TurboAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TurboAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="TurboAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
